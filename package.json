    "axios": "~0.21.1",
    "axios-observable": "~1.1.6",
    "codelyzer": "6.0.1",
    "dotenv": "8.2.0",
    "eslint": "7.24.0",
    "eslint-config-prettier": "8.2.0",
    "jest": "26.6.3",
